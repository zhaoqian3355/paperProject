                    <ul class="section menu">
                        <li>
                            <a asp-controller="City" asp-action="Index">城市数据集</a>
                        </li>
                        <li>
                            <a class="menuitem">站点数据集</a>
                            <ul class="submenu">
                                <li><a asp-controller="Train" asp-action="Station">火车</a></li>
                                <li><a asp-controller="Flight" asp-action="Station">飞机</a></li>
                                <li><a asp-controller="Ship" asp-action="Station">轮船</a></li>
                            </ul>
                        </li>
                        <li>
                            <a class="menuitem">线路数据集</a>
                            <ul class="submenu">
                                <li><a asp-controller="Train" asp-action="Line">火车</a></li>
                                <li><a asp-controller="Flight" asp-action="Line">飞机</a></li>
                                <li><a asp-controller="Ship" asp-action="Line">轮船</a></li>
                            </ul>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
        <div class="grid_10">
            <div class="box round first grid">
                <h2>
                    @ViewData["Title"]
                </h2>
                <div class="block">
                    @RenderBody()
                </div>
            </div>
        </div>
        <div class="clear">
        </div>
