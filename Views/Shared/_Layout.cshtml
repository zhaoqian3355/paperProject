    <script src="~/js/setup.js" type="text/javascript"></script>
    <script type="text/javascript">

        $(document).ready(function () {
            setupLeftMenu();

            $('.datatable').dataTable();
            setSidebarHeight();
            $("#example_info").text("1 到 10 条")

        });
    </script>
    <style>
        .prev-next{
            position: absolute;
            bottom: 49px;
            right: 100px;
        }
